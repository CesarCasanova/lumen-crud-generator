{
    "name": "ccasanova/lumen-crud-generator",
    "description": "Simple CRUD generation for Lumen",
    "license": "WTFPL",
    "type": "lumen-package",
    "keywords": [
        "crud",
        "crud generator",
        "lumen"
    ],
    "require": {
        "laravel/lumen-framework": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "WRonX\\LumenCrudGenerator\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WRonX\\LumenCrudGenerator\\Providers\\LumenCrudGeneratorProvider"
            ]
        }
    }
}
