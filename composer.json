{
    "name": "wronx/lumen-crud-generator",
    "description": "Simple CRUD generation for Lumen",
    "license": "WTFPL",
    "type": "lumen-package",
    "keywords": [
        "crud",
        "crud generator",
        "lumen"
    ],
    "require": {
        "flipbox/lumen-generator": "^5.6",
        "laravel/lumen-framework": "5.7.*",
        "wronx/lumen-rest-object-fetch-middleware": "^0.1.0"
    },
    "authors": [
        {
            "name": "Marcin \"WRonX\" Wronka",
            "email": "wronx@wronx.net",
            "homepage": "http://wronx.net",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "WRonX\\LumenCrudGenerator\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "WRonX\\LumenCrudGenerator\\Providers\\LumenCrudGeneratorProvider"
            ]
        }
    }
}
